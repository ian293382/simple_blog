<h4 class='d-flex justify-content-between md-4'>
  <span>Posts</span>
  <%= link_to 'New Post', new_post_path, class: 'btn btn-primary' %>
</h4>

<% @posts.each do |post| %>
<%= link_to  post do %>
  <div class="card md-3">
    <div class="card-body">
      <h5 class='card-title'><%= post.title %></h5>
    <p><%= post.description %></p>
    </div>
  </div>
  <% end %>
<% end %>
